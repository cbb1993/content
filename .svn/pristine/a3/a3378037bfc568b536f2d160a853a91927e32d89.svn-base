package com.huanhong.content.model.plan;

import com.google.gson.annotations.SerializedName;
import com.huanhong.content.model.split.Split;

import java.io.Serializable;
import java.util.List;

public class PlanContent implements Serializable
{
    private final boolean isPlanContent = true;

    private List<Split> split;

    @SerializedName("tasks")
    private List<SplitSchedule> mSplitSchedule;

    public List<Split> getSplit()
    {
        return split;
    }

    public void setSplit(List<Split> split)
    {
        this.split = split;
    }

    public List<SplitSchedule> getSplitSchedule()
    {
        return mSplitSchedule;
    }

    public void setSplitSchedule(List<SplitSchedule> splitSchedule)
    {
        this.mSplitSchedule = splitSchedule;
    }

    public boolean isLocal() {
        return isPlanContent;
    }
}
